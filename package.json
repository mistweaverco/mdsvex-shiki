{
  "name": "@mistweaverco/mdsvex-shiki",
  "version": "1.1.1",
  "type": "module",
  "main": "./index.cjs",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles.css": "./styles.css"
  },
  "files": [
    "dist/*.{js,cjs,d.ts,css}",
    "styles.css"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "bun run build",
    "clear": "rm -f *.js *.cjs *.d.ts",
    "build": "bun build.ts",
    "publish:npm": "npm publish --access public",
    "publish:github": "npm publish --registry https://npm.pkg.github.com --access public"
  },
  "keywords": [
    "code",
    "hightlight",
    "hightlights",
    "mdsvex",
    "prismjs",
    "shiki",
    "svelte",
    "sveltekit",
    "syntax"
  ],
  "author": "mistweaverco and friends <open-source@mistweaverco.com>",
  "license": "MIT",
  "description": "Shiki syntax highlighter for mdsvex",
  "private": false,
  "packageManager": "bun@1.3.3",
  "dependencies": {
    "@shikijs/transformers": "3.19.0",
    "shiki": "3.19.0"
  },
  "devDependencies": {
    "@eslint/json": "0.14.0",
    "@eslint/markdown": "7.5.1",
    "@types/bun": "1.3.3",
    "bun-plugin-dts": "0.3.0",
    "eslint": "9.39.1",
    "prettier": "3.7.4",
    "svelte": "5.45.10",
    "typescript-eslint": "8.49.0"
  },
  "peerDependencies": {
    "typescript": "5.9.3"
  }
}

